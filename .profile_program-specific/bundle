alias b="bundle"
alias be="bundle exec"
alias bi="bundle install"
alias bu="bundle update"
alias cuc="bundle exec cucumber"
alias rk="bundle exec rake"
alias rkap="bundle exec rake assets:precompile"
alias rkap-c='bundle exec rake assets:precompile && git add . && git commit -m "Compile assets"'
alias rkdm="bundle exec rake db:migrate"
alias rkjw="bundle exec rake jobs:work"
alias r="rails"
alias rc="rails c"
alias rr="rails runner"
alias rs="rails s"
alias ru="bundle exec rackup"
alias zeus=$(which -a zeus | grep -m1 -v bundle)
