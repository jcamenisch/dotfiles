alias zeus=$(which -a zeus | grep -m1 -v bundle)
alias z=zeus
alias zr='zeus rake'
alias zrm='zeus rake minitest:all'
alias zrmq='zeus rake minitest:all:quick'
alias zrt='zeus rake test'
alias zs='zeus start'
alias zt='zeus test'
alias zts='zeus test spec'
alias ztt='zeus test test'

alias kz='pgrep zeus | xargs kill'
alias kpt="ps aux | grep 'postgres.\\+_t[e]st' | ruby -ne 'puts \$_.split(/ +/)[1]' | xargs kill"
