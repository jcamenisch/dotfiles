c() {
  cd $(ls_match -mask="%1*" -first $1)
}

cdh() { # depends on ls_match from https://github.com/jcamenisch/ls_match
  cd $(ls_match -mask="$HOME/%1:$HOME/%1*" -first $1)
  pwd
}

if [[ -d ~/projects ]]; then
  cdp() { # depends on ls_match from https://github.com/jcamenisch/ls_match
    cd $(ls_match -mask="$HOME/projects/%1:$HOME/projects/%1*" -first $1)
    pwd
  }
fi
